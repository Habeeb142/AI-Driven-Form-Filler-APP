<?xml version='1.0' encoding='utf-8'?>
<widget id="com.FormFil.app" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:android="http://schemas.android.com/apk/res/android">
    <name>FillForm</name>
    <description>
        An App for filling form
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Mustapha and Adebayo
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <edit-config file="app/src/main/AndroidManifest.xml" mode="merge" target="/manifest/application">
            <application android:usesCleartextTraffic="true" />
        </edit-config>

        <icon src="src/assets/ic.jpeg" width="57" height="57" density="mdpi"/>
        <icon src="src/assets/ic.jpeg" width="57" height="57" density="xhdpi"/>
        <icon src="src/assets/ic.jpeg" width="57" height="57" density="xxhdpi"/>
        <icon src="src/assets/ic.jpeg" width="57" height="57" density="hdpi"/>
    </platform>
    <!-- <preference name="StatusBarBackgroundColor" value="#fff" /> -->
</widget>
